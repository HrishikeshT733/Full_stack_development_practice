package com.seed.enumDemo;

enum Coffeesize1
{
	SMALL,LARGE,MEDIUM
}
public class coffeeenum {

	Coffeesize1 size ;
	public static void main(String[] args)
	{
		
		coffeeenum c=new coffeeenum();
		System.out.println(Coffeesize1.SMALL);
		c.size=Coffeesize1.SMALL;
		
		System.out.println(c.size);
	}

}

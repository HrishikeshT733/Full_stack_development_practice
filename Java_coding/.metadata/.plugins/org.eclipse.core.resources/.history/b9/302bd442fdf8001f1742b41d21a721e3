package com.labmanual2;

import java.util.Scanner;

class Prob1 extends Thread{
	public Prob1(int a)
    {
		
		for(int i=0;i<5;i++)	
		{
	        a=a+i;
	        System.out.println(a);
		}	
    }
	public void run()
    {
		for(int i=0;i<3;i++)	
		{
			try{
				Thread.sleep(3000);
//				wait();
			}catch(InterruptedException e)
			{
				e.printStackTrace();
//				notify();
			}
			
		}	
    }
	
}
